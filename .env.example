# Voice Chat â€” Environment Variables
# Copy to .env and fill in values.

# --- STT (Parakeet) ---
PARAKEET_URL=http://100.86.69.14:8765

# --- TTS (Kokoro - local) ---
KOKORO_URL=http://100.86.69.14:8787

# --- TTS (OpenAI - cloud) ---
OPENAI_API_KEY=
# VOICECHAT_OPENAI_API_KEY=       # Alternative name
VOICECHAT_TTS_MODEL=gpt-4o-mini-tts
VOICECHAT_TTS_VOICE=nova

# --- OpenClaw Gateway ---
VOICECHAT_GATEWAY_URL=ws://127.0.0.1:18789/gateway
VOICECHAT_GATEWAY_TOKEN=
# OPENCLAW_GATEWAY_TOKEN=          # Alternative name
VOICECHAT_GATEWAY_PASSWORD=
# OPENCLAW_GATEWAY_PASSWORD=       # Alternative name
VOICECHAT_GATEWAY_PROTOCOL=3
VOICECHAT_GATEWAY_TIMEOUT_MS=120000
VOICECHAT_GATEWAY_ALLOW_INSECURE=false
VOICECHAT_GATEWAY_CLIENT_ID=webchat
VOICECHAT_GATEWAY_CLIENT_NAME=voice-chat-bridge
VOICECHAT_GATEWAY_CLIENT_MODE=
VOICECHAT_GATEWAY_DEVICE_ID=
VOICECHAT_GATEWAY_DEVICE_FILE=

# --- OpenClaw Config Paths ---
OPENCLAW_HOME=~/.openclaw
VOICECHAT_OPENCLAW_CONFIG=
VOICECHAT_OPENCLAW_DEVICE_FILE=
VOICECHAT_OPENCLAW_DEVICE_AUTH_FILE=
VOICECHAT_GATEWAY_DEVICE_WRITE=false

# --- Session Defaults ---
VOICECHAT_AGENT_ID=main
VOICECHAT_SESSION_KEY=main

# --- Bridge Server (legacy) ---
VOICE_BRIDGE_PORT=8787
